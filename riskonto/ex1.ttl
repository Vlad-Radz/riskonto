###  https://example.com/riskonto/ex#DB
:Database rdf:type owl:NamedIndividual ,
             risk:System ;
    risk:hasRisk :UnauthorisedAccess ;
    rdfs:label "Database"@en .


###  https://example.com/riskonto/ex#UnauthorisedAccess
:UnauthorisedAccess rdf:type owl:NamedIndividual ,
                             risk:Risk ;
                    rdfs:label "Unauthorised Access"@en .

:Person a risk:Agent ; rdfs:label "Person"@en .
:GuessPassword a risk:Source ; rdfs:label "Guess Password"@en ;
    risk:hasFrequencyOfOccurence risk:Certain ;
    risk:hasFrequencyOverPeriod risk:Day ;
    risk:hasLikelihood risk:Certain ;
    risk:hasProbability risk:Certain .
:GuessPasswordWithCaptcha a risk:Source ; rdfs:label "Guess Password"@en ;
    risk:hasFrequencyOfOccurence risk:Rare ;
    risk:hasFrequencyOverPeriod risk:Decade ;
    risk:hasLikelihood risk:ExtremelyRare ;
    risk:hasProbability risk:ExtremelyRare ;
    risk:hasAgent :Person ;
    risk:isModifiedFrom :GuessPassword .

:GuessPassword risk:hasAgent :Person .
:GuessPassword risk:exploitsVulnerability :WeakPasswordPolicy .
:WeakPasswordPolicy a risk:Vulnerability ; rdfs:label "Weak Password Policy"@en .
:Database risk:hasVulnerability :WeakPasswordPolicy .


###  Generated by the OWL API (version 4.5.9.2019-02-01T07:24:44Z) https://github.com/owlcs/owlapi

:DetectPasswordGuessing a :DetectionControl ; rdfs:label "Detect Password Guessing"@en .

:PreventPasswordGuessing a :ModificationControl ; rdfs:label "Prevent Password Guessing"@en ;
    risk:hasSource :GuessPassword .
